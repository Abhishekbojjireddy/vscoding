<aura:component controller="Getaccountdetail" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
	<aura:attribute name ="AccountNumber" type="String"/>
    <aura:attribute name ="Name" type="String"/>
    
    <lightning:input type="AccountNumber" label="AccountNo"
            placeholder="1243567" value="{!v.AccountNumber}"/>
    
    <lightning:button label="Showname" title="Neutral action" onclick="{! c.accClick }"/>
    <lightning:input label="AccountName" value="{!v.Name}"/>
</aura:component>